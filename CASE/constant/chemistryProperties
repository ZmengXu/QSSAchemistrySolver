FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    location        "constant";
    object          chemistryProperties;
}

chemistryType
{
    chemistrySolver QSS;
    method          standard;
}

chemistry       on;

initialChemicalTimeStep 1e-07;

EulerImplicitCoeffs
{
    cTauChem        1;
    equilibriumRateLimiter off;
}

odeCoeffs
{
    solver          seulex;
    absTol          1e-12;
    relTol          0.1;
}

QSSCoeffs
{
    solver          seulex;
    absTol          1e-12;
    relTol          0.1;
}


